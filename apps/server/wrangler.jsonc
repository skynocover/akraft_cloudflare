{
    "name": "akraft-cloudflare",
    "compatibility_date": "2025-12-14",
    "main": "src/index.ts",
    "compatibility_flags": ["nodejs_compat"],
    "observability": {
        "enabled": true
    },
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "akraft-db",
            "database_id": "local"
        }
    ],
    "r2_buckets": [
        {
            "binding": "R2",
            "bucket_name": "akraft-images"
        }
    ]
}